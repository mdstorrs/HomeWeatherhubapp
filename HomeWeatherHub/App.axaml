<Application
    x:Class="HomeWeatherHub.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    RequestedThemeVariant="Default">
    <!--  "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options.  -->

    <Application.Styles>
        <FluentTheme />
      <StyleInclude Source="avares://HomeWeatherHub/Styles/Light.axaml" />
      <StyleInclude Source="avares://HomeWeatherHub/Styles/Dark.axaml" />      
    </Application.Styles>



</Application>
